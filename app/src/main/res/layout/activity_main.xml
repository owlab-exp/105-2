        -->

    <com.example.android.calendartest6.TimeLineView
        xmlns:tp="http://schemas.android.com/apk/res-auto"
        android:id="@+id/timePlane"
        tp:centerLineThickness="1dp"
        tp:centerLineColor="#FF7F6E"
        tp:timeLabelTextSize="14sp"
        tp:timeLabelTextColor="#555555"
        tp:timeLineThickness="1dp"
        tp:timeLineColor="#DDDDDD"
        tp:halfTimeLineThickness="1dp"
        tp:halfTimeLineColor="#f0f2ec"

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

</android.support.design.widget.CoordinatorLayout>

